algoritmo "Exercício A"
var

A: vetor[1..10] de caractere
C: inteiro
inicio

      //ENTRADA DE DADOS DO VENTOR A

      Para C := 1 ate 10 faca
           Leia(A[C])
           LimpaTela
      FimPara
      
      // SAÍDA DE DADOS DO VETOR A
      
      Para C := 1 ate 10 faca
           EscrevaL(A[C])
      FimPara

fimalgoritmo